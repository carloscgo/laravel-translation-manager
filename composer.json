{
    "name": "carloscgo/laravel-translation-manager",
    "description": "Manage Laravel Translations",
    "keywords": ["laravel", "translations", "translator"],
    "license": "MIT",
    "authors": [
        {
            "name": "Carlos Camacho",
            "email": "carloscgo123@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "5.0.x|5.1.x|5.2.x|5.3.x|5.4.x|5.5.x|5.6.x|5.7.x",
        "illuminate/translation": "5.0.x|5.1.x|5.2.x|5.3.x|5.4.x|5.5.x|5.6.x|5.7.x",
        "symfony/finder": "~2.3|~3.0|~4.0"
    },
    "autoload": {
        "psr-4": {
            "CarlosCGO\\TranslationManager\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.4-dev"
        },
        "laravel": {
            "providers": [
                "CarlosCGO\\TranslationManager\\ManagerServiceProvider"
            ]
        }
    }
}
